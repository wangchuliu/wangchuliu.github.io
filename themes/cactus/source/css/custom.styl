.indianred
  color: indianred

.gray
  color: #666

.box
  border: dashed 1px #666
  padding: 1em

.underline
  border-bottom: dashed 1px #666
  padding: 0.5em

.lower-alpha+ol, .lower-alpha+ul
  list-style-type: lower-alpha

ol > li > h3
  margin: 0

.green
  color: rgba(86,124,119,0.8)

.bold
  font-weight: bold
